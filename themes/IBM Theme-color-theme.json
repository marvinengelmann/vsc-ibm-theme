{
    "name": "IBM Theme",
    "type": "dark",
    "colors": {
        "foreground": "#c6c6c6",
        "activityBar.background": "#161616",
        "activityBar.foreground": "#ffffff",
        "activityBar.inactiveForeground": "#a8a8a8",
        "activityBar.activeBorder": "#0f62fe",
        "activityBar.activeBackground": "#393939",
        "activityBar.border": "#393939",
        "activityBarBadge.background": "#0f62fe",
        "sideBar.background": "#161616",
        "sideBar.border": "#393939",
        "list.hoverBackground": "#393939",
        "list.hoverForeground": "#ffffff",
        "list.activeSelectionBackground": "#393939",
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionBackground": "#393939",
        "list.inactiveSelectionForeground": "#ffffff",
        "list.warningForeground": "#f1c21b",
        "list.errorForeground": "#da1e28",
        "gitDecoration.modifiedResourceForeground": "#8a3ffc",
        "editorGroupHeader.tabsBackground": "#161616",
        "editorGroupHeader.tabsBorder": "#393939",
        "tab.activeBackground": "#393939",
        "tab.inactiveBackground": "#161616",
        "tab.activeBorder": "#0f62fe",
        "tab.border": "#0000",
        "statusBar.background": "#0f62fe",
        "statusBar.noFolderBackground": "#0f62fe",
        "statusBar.debuggingBackground": "#8a3ffc",
        "button.background": "#0f62fe",
        "button.hoverBackground": "#0353e9",
        "extensionButton.prominentBackground": "#0f62fe",
        "extensionButton.prominentHoverBackground": "#0353e9",
        "inputOption.activeBorder": "#0f62fe",
        "editor.background": "#161616",
        "editor.foreground": "#ffffff",
        "editorLineNumber.foreground": "#c6c6c6"
    },
    "tokenColors": [
        {
            "name": "Red 40",
            "scope": [
                // Global
                "keyword.operator.pattern",
                "keyword.operator.combinator",
                "keyword.operator.arithmetic",
                "keyword.operator.bitwise.shift",
                "keyword.operator.logical",
                "keyword.operator.comparison",
                "keyword.operator.relational",
                "keyword.operator.increment",
                "keyword.operator.decrement",
                "keyword.operator.optional",
                "keyword.operator.increment-decrement",
                "keyword.operator.nowdoc",
                "keyword.operator.heredoc",
                "keyword.operator.null-coalescing",
                "keyword.operator.null-conditional",
                "keyword.operator.conditional.question-mark",
                "keyword.operator.conditional.colon",
                "storage.modifier.reference",
                "punctuation.separator.question-mark"
            ],
            "settings": {
                "foreground": "#ff8389"
            }
        },
        {
            "name": "Magenta 40",
            "scope": [
                // Global
                "entity.name.tag.wildcard",
                "keyword.control",                
                "keyword.operator.logical.and.media",
                "keyword.operator.expression.in",
                "keyword.operator.expression.of",
                "keyword.operator.new",
                "keyword.operator.expression.typeof",
                "keyword.other.important",
                "keyword.other.function.use",
                "keyword.other.new",
                "keyword.other.use",
                "keyword.other.namespace",
                "keyword.other.use-as",
                "keyword.other.using",
                "variable.language.super",
                "constant.character.escape",

                // CSS
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#ff7eb6"
            }
        },
        {
            "name": "Purple 50",
            "scope": [
                // Global
                "entity.other.attribute-name.pseudo-class",
                "entity.other.attribute-name.pseudo-element",
                "entity.other.inherited-class",
                "entity.other.alias",
                "entity.name.type",
                "entity.name.variable.event",
                "support.constant.property-value",
                "support.constant.color.w3c-extended-color-name",
                "support.constant.color.w3c-standard-color-name",
                "support.constant.font-name",
                "support.constant.math",
                "support.class",
                "support.type.primitive",
                "support.other.namespace",
                "constant.language",

                // C#
                "storage.type.cs",

                // Markdown
                "meta.separator.markdown"                
            ],
            "settings": {
                "foreground": "#a56eff"
            }
        },
        {
            "name": "Blue 50",
            "scope": [
                // Global
                "entity.name.tag",
                "entity.other.attribute-name.class",
                "entity.other.attribute-name.id",
                "support.constant.media",
                "storage.type",
                "storage.modifier",
                "variable.language.this",
                "keyword.type",
                "keyword.other.class",
                "keyword.other.var",
                "keyword.other.delegate",
                "keyword.other.event",
                "keyword.other.enum",
                "keyword.other.this",
                "keyword.other.get",
                "keyword.other.set",
                "keyword.other.interface",
                "keyword.other.nameof",
                "keyword.other.static",

                // JSON
                "support.type.property-name.json",

                // Markdown
                "punctuation.definition.heading.markdown",
                "markup.heading.setext.1.markdown",
                "markup.heading.setext.2.markdown",
                "punctuation.definition.quote.begin.markdown",
                "punctuation.definition.list.begin.markdown"
            ],
            "settings": {
                "foreground": "#4589ff"
            }
        },
        {
            "name": "Cyan 40",
            "scope": [
                // Global
                "entity.other.attribute-name",
                "entity.name.label",
                "entity.name.variable.parameter",
                "entity.name.variable.local",
                "entity.name.variable.field",
                "entity.name.variable.enum-member",
                "entity.name.variable.property",
                "entity.name.variable.tuple-element",
                "support.type.property-name",
                "support.variable.property",
                "variable.other",
                "variable.parameter",
                "variable.object.property",
                "meta.object-literal.key",
                "constant.other",

                // Markdown
                "punctuation.definition.italic.markdown",
                "punctuation.definition.bold.markdown",
                "constant.character.escape.markdown"
            ],
            "settings": {
                "foreground": "#33b1ff"
            }
        },
        {
            "name": "Teal 40",
            "scope": [
                // Global
                "support.function",
                "keyword.other.unit",
                "keyword.other.base",
                "entity.name.function",

                // Markdown
                "markup.inline.raw.string.markdown"
            ],
            "settings": {
                "foreground": "#08bdba"
            }
        },
        {
            "name": "Green 30",
            "scope": [
                // Global
                "constant.numeric",
                "constant.other.color.rgb-value.hex"
            ],
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "name": "Gray 70",
            "scope": "comment",
            "settings": {
                "foreground": "#525252"
            }
        },
        {
            "name": "Gray 40",
            "scope": [
                // Global
                "string.quoted.double",
                "string.quoted.single",
                "string.template",
                "string.unquoted.nowdoc",
                "string.unquoted.heredoc",
                "variable.parameter.url",
                
                // Markdown
                "markup.underline.link.markdown",
                "markup.underline.link.image.markdown",
                "string.other.link.description.title.markdown",
                "string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#a8a8a8"
            }
        },
        {
            "name": "White",
            "scope": [
                // Global
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",
                "punctuation.definition.entity",
                "punctuation.definition.string.template.begin",
                "punctuation.definition.string.template.end",
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "punctuation.definition.variable",
                "punctuation.definition.string",
                "punctuation.definition.interpolation.begin",
                "punctuation.definition.interpolation.end",
                "punctuation.definition.raw",
                "punctuation.support.type.property-name.begin",
                "punctuation.support.type.property-name.end",
                "punctuation.separator.inheritance",
                "storage.type.function.arrow"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "name": "Italic",
            "scope": [
                // Global
                "entity.other.attribute-name",
                "entity.name.tag",
                "keyword.control",                
                "keyword.operator.logical.and.media",
                "keyword.operator.expression.in",
                "keyword.operator.expression.of",
                "keyword.operator.expression.typeof",
                "keyword.operator.new",
                "keyword.other",              
                "keyword.type",
                "support.constant.color.w3c-extended-color-name",
                "support.constant.color.w3c-standard-color-name",
                "support.type.primitive",
                "constant.language",
                "storage.type",
                "storage.modifier",
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "None",
            "scope": [
                // Global
                "storage.type.function.arrow",

                // CSS
                "entity.other.attribute-name.class.css",

                // C#
                "storage.type.cs"
            ],
            "settings": {
                "fontStyle": ""
            }
        }                                     
    ]
}