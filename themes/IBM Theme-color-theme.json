{
    "name": "IBM Theme",
    "type": "dark",
    "colors": {
        "foreground": "#c6c6c6",
        "activityBar.background": "#161616",
        "activityBar.foreground": "#ffffff",
        "activityBar.inactiveForeground": "#a8a8a8",
        "activityBar.activeBorder": "#0f62fe",
        "activityBar.activeBackground": "#393939",
        "activityBar.border": "#393939",
        "activityBarBadge.background": "#0f62fe",
        "sideBar.background": "#161616",
        "sideBar.border": "#393939",
        "list.hoverBackground": "#393939",
        "list.hoverForeground": "#ffffff",
        "list.activeSelectionBackground": "#393939",
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionBackground": "#393939",
        "list.inactiveSelectionForeground": "#ffffff",
        "list.warningForeground": "#f1c21b",
        "list.errorForeground": "#da1e28",
        "gitDecoration.modifiedResourceForeground": "#8a3ffc",
        "editorGroupHeader.tabsBackground": "#161616",
        "editorGroupHeader.tabsBorder": "#393939",
        "tab.activeBackground": "#393939",
        "tab.inactiveBackground": "#161616",
        "tab.activeBorder": "#0f62fe",
        "tab.border": "#0000",
        "statusBar.background": "#0f62fe",
        "statusBar.debuggingBackground": "#8a3ffc",
        "statusBar.noFolderBackground": "#007d79",
        "button.background": "#0f62fe",
        "button.hoverBackground": "#0353e9",
        "extensionButton.prominentBackground": "#0f62fe",
        "extensionButton.prominentHoverBackground": "#0353e9",
        "inputOption.activeBorder": "#0f62fe",
        "editor.background": "#161616",
        "editor.foreground": "#ffffff",
        "editorLineNumber.foreground": "#c6c6c6"
    },
    "tokenColors": [
        {
            "name": "Red 40",
            "scope": [
                "keyword.operator.arithmetic",
                "keyword.operator.comparison",
                "keyword.operator.logical",
                "keyword.operator.combinator.css",
                "keyword.operator.pattern.css",
                "keyword.operator.bitwise.shift.js",
                "keyword.operator.relational.js",
                "keyword.operator.increment.js",
                "keyword.operator.decrement.js",
                "keyword.operator.optional.ts",
                "keyword.operator.nowdoc.php",
                "keyword.operator.heredoc.php",
                "storage.modifier.reference.php",
                "keyword.operator.ternary.php",
                "keyword.operator.null-coalescing.php",
                "keyword.control.endif.php",
                "keyword.operator.increment-decrement.php"
            ],
            "settings": {
                "foreground": "#ff8389"
            }
        },
        {
            "name": "Magenta 40",
            "scope": [
                "keyword.control.flow",
                "keyword.operator.new",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.operator.expression.of",
                "keyword.operator.expression.in",
                "keyword.control.switch",
                "entity.name.tag.css",
                "entity.name.tag.wildcard.css",
                "keyword.other.unit.percentage.css",
                "keyword.other.unit.em.css",
                "keyword.other.unit.rem.css",
                "keyword.other.unit.vw.css",
                "keyword.other.unit.vh.css",
                "keyword.other.unit.vmin.css",
                "keyword.other.unit.vmax.css",
                "keyword.other.unit.px.css",
                "keyword.other.unit.pt.css",
                "keyword.other.unit.cm.css",
                "keyword.other.unit.mm.css",
                "keyword.other.unit.in.css",
                "keyword.other.important.css",
                "keyword.operator.expression.typeof.js",                
                "variable.language.super.ts",
                "keyword.control.export.ts",
                "keyword.control.import.ts",
                "keyword.control.as.ts",
                "keyword.control.if.php",
                "keyword.control.else.php",
                "keyword.control.elseif.php",
                "keyword.control.case.php",
                "keyword.control.break.php",
                "keyword.control.default.php",
                "keyword.control.while.php",
                "keyword.control.do.php",
                "keyword.control.for.php",
                "keyword.control.foreach.php",
                "keyword.control.continue.php",
                "keyword.control.return.php",
                "keyword.control.import.include.php",
                "keyword.other.new.php",
                "keyword.other.use.php",
                "keyword.other.namespace.php",
                "keyword.other.use-as.php",
                "keyword.control.exception.php",
                "keyword.control.exception.catch.php"
            ],
            "settings": {
                "foreground": "#ff7eb6"
            }
        },
        {
            "name": "Purple 50",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other.color.rgb-value.hex.css"
            ],
            "settings": {
                "foreground": "#a56eff"
            }
        },
        {
            "name": "Blue 50",
            "scope": [
                "storage.type",
                "storage.modifier",
                "variable.language.this",
                "entity.name.tag.html",
                "entity.other.attribute-name.class.css",                
                "entity.other.attribute-name.id.css",
                "support.type.property-name.json"
                "support.function.magic.php"
            ],
            "settings": {
                "foreground": "#4589ff"
            }
        },
        {
            "name": "Cyan 40",
            "scope": [
                "variable.other.readwrite",
                "variable.other.constant",
                "variable.parameter",
                "meta.object-literal.key",
                "variable.other.object",
                "variable.other.property",
                "support.variable.property",
                "entity.other.attribute-name.html",
                "support.type.property-name.css",
                "entity.other.attribute-name.css",
                "variable.other.object.property.js",
                "variable.object.property.ts",
                "variable.other.enummember.ts",
                "variable.other.php",
                "constant.other.php",
                "constant.other.class.php",
                "variable.other.class.php"
            ],
            "settings": {
                "foreground": "#33b1ff"
            }
        },
        {
            "name": "Teal 30",
            "scope": [
                "entity.name.function",
                "support.function.math",
                "entity.other.attribute-name.pseudo-class.css",
                "support.constant.font-name.css",
                "support.constant.property-value.css",
                "support.constant.color.w3c-extended-color-name.css",
                "support.constant.color.w3c-standard-color-name.css",
                "entity.other.attribute-name.pseudo-element.css",
                "support.function.js",
                "support.function.construct.output.php",
                "support.function.construct.php",
                "support.function.basic_functions.php",
                "support.function.array.php",
                "support.function.var.php",
                "support.function.info.php",
                "support.function.funchand.php",
            ],
            "settings": {
                "foreground": "#08bdba"
            }
        },
        {
            "name": "Green 30",
            "scope": [
                "entity.name.type",
                "support.constant.math",
                "support.class",
                "support.function.misc.css",
                "support.function.url.css",
                "entity.other.inherited-class.ts",
                "support.type.primitive.ts",
                "entity.other.inherited-class.php",
                "support.other.namespace.php",
                "entity.other.alias.php"
            ],
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "name": "Gray 70",
            "scope": "comment",
            "settings": {
                "foreground": "#525252"
            }
        },
        {
            "name": "Gray 40",
            "scope": [
                "string.quoted.double",
                "string.quoted.single",
                "string.template",
                "variable.parameter.url.css",
                "string.unquoted.nowdoc.php",
                "string.unquoted.heredoc.php"
            ],
            "settings": {
                "foreground": "#a8a8a8"
            }
        },
        {
            "name": "White",
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",
                "punctuation.definition.string.template.begin",
                "punctuation.definition.string.template.end",
                "punctuation.definition.template-expression.begin",
                "punctuation.definition.template-expression.end",
                "storage.type.function.arrow",
                "punctuation.definition.entity.css",
                "punctuation.support.type.property-name.begin.json",
                "punctuation.support.type.property-name.end.json",
                "punctuation.separator.key-value.js",
                "punctuation.definition.variable.php",
                "punctuation.definition.string.php"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        }                                          
    ]
}