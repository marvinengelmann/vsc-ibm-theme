{
    "name": "IBM Theme",
    "type": "dark",
    "colors": {
        "foreground": "#c6c6c6",
        "activityBar.background": "#161616",
        "activityBar.foreground": "#ffffff",
        "activityBar.inactiveForeground": "#a8a8a8",
        "activityBar.activeBorder": "#0f62fe",
        "activityBar.activeBackground": "#393939",
        "activityBar.border": "#393939",
        "activityBarBadge.background": "#0f62fe",
        "sideBar.background": "#161616",
        "sideBar.border": "#393939",
        "list.hoverBackground": "#393939",
        "list.hoverForeground": "#ffffff",
        "list.activeSelectionBackground": "#393939",
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionBackground": "#393939",
        "list.inactiveSelectionForeground": "#ffffff",
        "list.warningForeground": "#f1c21b",
        "list.errorForeground": "#da1e28",
        "gitDecoration.modifiedResourceForeground": "#8a3ffc",
        "editorGroupHeader.tabsBackground": "#161616",
        "editorGroupHeader.tabsBorder": "#393939",
        "tab.activeBackground": "#393939",
        "tab.inactiveBackground": "#161616",
        "tab.activeBorder": "#0f62fe",
        "tab.border": "#0000",
        "statusBar.background": "#0f62fe",
        "statusBar.debuggingBackground": "#8a3ffc",
        "statusBar.noFolderBackground": "#007d79",
        "button.background": "#0f62fe",
        "button.hoverBackground": "#0353e9",
        "extensionButton.prominentBackground": "#0f62fe",
        "extensionButton.prominentHoverBackground": "#0353e9",
        "inputOption.activeBorder": "#0f62fe",
        "editor.background": "#161616",
        "editor.foreground": "#ffffff",
        "editorLineNumber.foreground": "#c6c6c6"
    },
    "tokenColors": [
        {
            "name": "Red 40",
            "scope": [
                "keyword.operator.combinator.css",
                "keyword.operator.pattern.css",
                "keyword.operator.arithmetic.js",
                "keyword.operator.bitwise.shift.js",
                "keyword.operator.logical.js",
                "keyword.operator.comparison.js",
                "keyword.operator.relational.js",
                "keyword.operator.assignment.compound.js",
                "keyword.operator.increment.js",
                "keyword.operator.decrement.js",                
            ],
            "settings": {
                "foreground": "#ff8389"
            }
        },
        {
            "name": "Magenta 40",
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.wildcard.css",
                "keyword.other.unit.percentage.css",
                "keyword.other.unit.em.css",
                "keyword.other.unit.rem.css",
                "keyword.other.unit.vw.css",
                "keyword.other.unit.vh.css",
                "keyword.other.unit.vmin.css",
                "keyword.other.unit.vmax.css",
                "keyword.other.unit.px.css",
                "keyword.other.unit.pt.css",
                "keyword.other.unit.cm.css",
                "keyword.other.unit.mm.css",
                "keyword.other.unit.in.css",
                "keyword.control.conditional.js",
                "keyword.control.loop.js",
                "keyword.operator.expression.in.js",
                "keyword.operator.expression.of.js",
                "keyword.control.switch.js",
                "keyword.control.flow.js",
                "keyword.operator.new.js",
                "keyword.operator.expression.typeof.js"
            ],
            "settings": {
                "foreground": "#ff7eb6"
            }
        },
        {
            "name": "Purple 50",
            "scope": [
                "constant.numeric",
                "constant.language",
                "constant.other.color.rgb-value.hex.css"
            ],
            "settings": {
                "foreground": "#a56eff"
            }
        },
        {
            "name": "Blue 50",
            "scope": [
                "entity.name.tag.html",
                "entity.other.attribute-name.class.css",                
                "entity.other.attribute-name.id.css",
                "support.type.property-name.json",
                "storage.type.js",
                "storage.type.function.js",
                "variable.language.this.js"
            ],
            "settings": {
                "foreground": "#4589ff"
            }
        },
        {
            "name": "Cyan 40",
            "scope": [
                "entity.other.attribute-name.html",
                "support.type.property-name.css",
                "entity.other.attribute-name.css",
                "support.variable.property.js",
                "variable.other.readwrite.js",
                "variable.other.object.js",
                "variable.other.property.js",
                "meta.object-literal.key.js",
                "variable.parameter.js",
                "variable.other.object.property.js",
                "variable.other.constant.js"
            ],
            "settings": {
                "foreground": "#33b1ff"
            }
        },
        {
            "name": "Teal 30",
            "scope": [
                "entity.other.attribute-name.pseudo-class.css",
                "support.constant.font-name.css",
                "support.constant.property-value.css",
                "support.constant.color.w3c-extended-color-name.css",
                "support.constant.color.w3c-standard-color-name.css",
                "entity.other.attribute-name.pseudo-element.css",
                "entity.name.function.js",
                "support.function.js",
                "support.function.math.js"
            ],
            "settings": {
                "foreground": "#08bdba"
            }
        },
        {
            "name": "Green 30",
            "scope": [
                "support.function.misc.css",
                "support.function.url.css",
                "support.constant.math.js",
                "entity.name.type.js",
                "support.class.builtin.js",
                "support.class.js"
            ],
            "settings": {
                "foreground": "#6fdc8c"
            }
        },
        {
            "name": "Gray 80",
            "scope": "comment",
            "settings": {
                "foreground": "#393939"
            }
        },
        {
            "name": "Gray 50",
            "scope": [
                "string.quoted.double",
                "string.quoted.single",
                "variable.parameter.url.css"
            ],
            "settings": {
                "foreground": "#8d8d8d"
            }
        },
        {
            "name": "White",
            "scope": [
                "punctuation.definition.string.begin",
                "punctuation.definition.string.end",
                "punctuation.definition.entity.css",
                "punctuation.support.type.property-name.begin.json",
                "punctuation.support.type.property-name.end.json",
                "punctuation.separator.key-value.js"
            ],
            "settings": {
                "foreground": "#ffffff"
            }
        }                                          
    ]
}