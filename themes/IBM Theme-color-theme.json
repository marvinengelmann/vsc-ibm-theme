    "tab.activeBorder": "#0f62fe",
    "tab.border": "#0000",
    "statusBar.background": "#0f62fe",
    "statusBar.debuggingBackground": "#8a3ffc",
    "button.background": "#0f62fe",
    "button.hoverBackground": "#0353e9",
    "extensionButton.prominentBackground": "#0f62fe",
    "extensionButton.prominentHoverBackground": "#0353e9",
    "inputOption.activeBorder": "#0f62fe",
    "editor.background": "#161616",
    "editor.foreground": "#c6c6c6",
    "editorLineNumber.foreground": "#c6c6c6"
	},
	"tokenColors": [
    {
      "name": "Gray 80",
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "#393939"
      }
    },
    {
      "name": "White",
      "scope": [
        "variable",
        "string.unquoted"
      ],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {      
      "name": "Magenta 40",
      "scope": [
        "keyword",
      ],
      "settings": {
        "foreground": "#ff7eb6"
      }
    },
    {      
      "name": "Purple 50",
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "#a56eff"
      }
    },
    {      
      "name": "Blue 50",
      "scope": [
        "storage"
      ],
      "settings": {
        "foreground": "#4589ff"
      }
    },
    {      
      "name": "Cyan 40",
      "scope": [
        "entity"
      ],
      "settings": {
        "foreground": "#33b1ff"
      }
    },
    {      
      "name": "Teal 30",
      "scope": [
        "string"
      ],
      "settings": {
        "foreground": "#3ddbd9"
      }
    },
    {      
      "name": "Green 30",
      "scope": [
        "support"
      ],
      "settings": {
        "foreground": "#6fdc8c"
      }
    },  
    {
      "name": "Italic",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator.instanceof",
        "constant.language",
        "keyword.operator.new",
        "keyword.operator.in",
        "keyword.other.namespace",
        "keyword.other.use",
        "keyword.other.class",
        "keyword.other.type",
        "variable.language.this"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
	]
}